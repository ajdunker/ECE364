def main():
    nums = input("Please enter some values: ").split()
    total = 0
    for num in nums:
        try:
            total += float(num)
        except ValueError:
            pass
        finally:
            pass
    print("The sum is: {}".format(total))

if __name__ == "__main__":
    main()
